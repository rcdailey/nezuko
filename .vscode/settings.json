{
  "yaml.schemas": {
    "https://json.schemastore.org/kustomization.json": "_kubernetes/**/kustomization.{yml,yaml}",
    "kubernetes": ["_kubernetes/**/*.yaml", "!_kubernetes/**/kustomization.{yml,yaml}"]
  },
  "files.associations": {
    "_kubernetes/**/*.yaml": "yaml",
    "_kubernetes/**/kustomization.yaml": "yaml"
  },
  "[yaml]": {
    "editor.snippetSuggestions": "none"
  },
  "yaml.schemaStore.enable": true
}
